<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.willhero.PauseMenu">
   <children>
      <ImageView fitHeight="422.0" fitWidth="712.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/PAUSE_Menu.jpg" />
         </image>
      </ImageView>
      <Button fx:id="settingsButton" layoutX="7.0" layoutY="5.0" mnemonicParsing="false" onAction="#setSettingsButton" opacity="0.0" prefHeight="52.0" prefWidth="52.0" text="settings" />
      <Button fx:id="cross" layoutX="663.0" layoutY="11.0" mnemonicParsing="false" onAction="#setCross" opacity="0.0" prefHeight="44.0" prefWidth="42.0" text="resume" />
      <Button fx:id="restart" layoutX="107.0" layoutY="128.0" mnemonicParsing="false" onAction="#setRestart" opacity="0.0" prefHeight="94.0" prefWidth="90.0" text="Restart" />
      <Button layoutX="312.0" layoutY="139.0" mnemonicParsing="false" opacity="0.0" prefHeight="86.0" prefWidth="90.0" text="SaveGame" />
      <Button fx:id="LoadSavedSavedGamesButton" layoutX="515.0" layoutY="132.0" mnemonicParsing="false" onAction="#setLoadSavedSavedGamesButton" opacity="0.0" prefHeight="94.0" prefWidth="90.0" text="LoadGame" />
      <Button fx:id="returnToMainMenu" layoutX="210.0" layoutY="241.0" mnemonicParsing="false" onAction="#setReturnToMainMenu" opacity="0.0" prefHeight="94.0" prefWidth="90.0" text="ReturnMenu" />
      <Button fx:id="exit" layoutX="424.0" layoutY="245.0" mnemonicParsing="false" onAction="#setExit" opacity="0.0" prefHeight="86.0" prefWidth="82.0" text="Exit" />
   </children>
</AnchorPane>
