<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.willhero.LoadSavedGamesMenu">
   <children>
      <ImageView fitHeight="401.0" fitWidth="715.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/savedgames.png" />
         </image>
      </ImageView>
      <Button fx:id="returnToMainMenu" layoutX="229.0" layoutY="254.0" mnemonicParsing="false" onAction="#setReturnToMainMenu" opacity="0.0" prefHeight="92.0" prefWidth="82.0" text="ReturnMenu" />
      <Button fx:id="exit" layoutX="441.0" layoutY="254.0" mnemonicParsing="false" onAction="#setExit" opacity="0.0" prefHeight="92.0" prefWidth="82.0" text="exit" />
      <Button fx:id="cross" layoutX="663.0" layoutY="14.0" mnemonicParsing="false" onAction="#setCross" opacity="0.0" prefHeight="44.0" prefWidth="44.0" text="Cross" />
   </children>
</AnchorPane>
